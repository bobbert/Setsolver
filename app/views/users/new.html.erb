<h1>New user</h1>

<%= error_messages_for :user %>

<% form_for(@user) do |f| %>
  <p>
    <b>Fname</b><br />
    <%= f.text_field :fname %>
  </p>

  <p>
    <b>Lname</b><br />
    <%= f.text_field :lname %>
  </p>

  <p>
    <b>Rating</b><br />
    <%= f.text_field :rating %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', users_path %>
